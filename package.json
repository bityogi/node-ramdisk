{
  "name": "node-ramdisk",
  "version": "1.1.1",
  "description": "create a ram disk with node.js",
  "main": "index.js",
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "scripts"
  ],
  "directories": [
    "scripts"
  ],
  "scripts": {
    "test": "lab -m 5000 -v -c test.js",
